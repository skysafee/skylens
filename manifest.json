{
  "name": "SkyLens",
  "short_name": "SkyLens",
  "description": "Ankuuu",
  "start_url": "/skylens/",
  "scope": "/skylens/",
  "display": "standalone",
  "orientation": "portrait",
  "background_color": "#0f1724",
  "theme_color": "#4caf50",
  "icons": [
    {
      "src": "/skylens/iconlensnew192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/skylens/iconlensnew512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any maskable"
    }
  ],
  "share_target": {
    "action": "/skylens/share-handler",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "file",
          "accept": [
            "image/*",
            "video/*"
          ]
        }
      ]
    }
  }
}
